@{
    ViewBag.Title = "Estudiante";
}
<div class="panel-heading bg-white">
    <h2>@ViewBag.Title</h2>
</div>
<br />

<div class="row">
    <div class="col-lg-4 col-md-6">
        <div class="panel" style="border: 1px solid #DEDEDE; border-left: 4px solid rgb(33, 150, 243);">
            <div class="panel-body">
                <div class="row">
                    <div class="col-xs-3">
                        @*<span class="icons icon-note"></span>*@
                        <i class="icons icon-note fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        @*<div class="huge">26</div>*@
                        <div><strong>Examenes</strong></div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("Exams", "Evaluations")">
                <div class="panel-footer">
                    <span class="pull-left">Tomar Examen</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-4 col-md-6">
        <div class="panel" style="border: 1px solid #DEDEDE; border-left: 4px solid rgb(33, 150, 243);">
            <div class="panel-body">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="icons icon-chart fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        @*<div class="huge">12</div>*@
                        <div><strong>Historial de Examenes</strong></div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("Results", "Evaluations")">
                <div class="panel-footer">
                    <span class="pull-left">Ver Historial</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-4 col-md-6">
        <div class="panel" style="border: 1px solid #DEDEDE; border-left: 4px solid rgb(33, 150, 243);">
            <div class="panel-body">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="icons icon-grid fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        @*<div class="huge">12</div>*@
                        <div><strong>Nueva Suscripción</strong></div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("Create", "StudentCareers")">
                <div class="panel-footer">
                    <span class="pull-left">Hacer Nueva Suscripción</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
</div>

<br />
@{ Html.RenderAction("StudentSuscriptions", "Students"); }



